+  gotoPref: function gPreferences__gotoPref(page) {
+    if (this.currentPage == page)
+      return;
+    this.search(page, "data-category");
+    window.history.pushState(page, document.title);
+    this.updateCommands();
+    this.currentPage = page;
+  },
+
+  commands: {
+    cmd_back: function gPreferences__cmd_back() {
+      window.history.back();
+    },
+    cmd_forward: function gPreferences__cmd_forward() {
+      window.history.forward();
+    }
+  },
+
+  doCommand: function gPreferences__doCommand(cmd) {
+    if (cmd in this.commands)
+      this.commands[cmd]();
+  },
+
+  onStatePopped: function gPreferences__onStatePopped(aEvent) {
+    var id = "category-"+aEvent.state.slice(4).toLowerCase();
+    var item = document.getElementById(id);
+    if (item) {
+      gPreferences.categories.suppressOnSelect = true;
+      gPreferences.categories.selectedItem = item;
+      gPreferences.categories.suppressOnSelect = false;
+    }
+    gPreferences.search(aEvent.state, "data-category");
+    gPreferences.currentPage = aEvent.state;
+    gPreferences.updateCommands();
+  },
+
+  updateCommands: function gPreferences__updateCommands() {
+    this.backButton.disabled = !this.canGoBack();
+    this.forwardButton.disabled = !this.canGoForward();
+  },
+
+  canGoBack: function gPreferences__canGoBack() {
+    return window.QueryInterface(Ci.nsIInterfaceRequestor)
+                 .getInterface(Ci.nsIWebNavigation)
+                 .canGoBack;
+  },
+
+  canGoForward: function gPreferences__canGoForward() {
+    return window.QueryInterface(Ci.nsIInterfaceRequestor)
+                 .getInterface(Ci.nsIWebNavigation)
+                 .canGoForward;
+  },
+
+  search: function gPreferences__search(aQuery, aAttribute) {
+    let elements = document.getElementById("mainPrefPane").children;
+    for (let element of elements) {
+      element.setAttribute("visible", (element.getAttribute(aAttribute) == aQuery));
+    }
+  }
 }
 
